<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- seekbar 배경 1 -->
    <item android:id="@+id/barBackground">
        <!-- 선 : 길이 -->
        <shape android:shape="line">
            <stroke android:width="5dp" android:color="@color/colorLightGray" />
        </shape>
    </item>

    <!-- seekbar 프로그래스 바 -->
    <item android:id="@+id/barProgress" >
        <clip>
            <shape android:shape="line">
                <stroke android:width="5dp" android:color="@color/teal_200" />
            </shape>
        </clip>
    </item>

</layer-list>